{
  "name": "Type.js",
  "version": "0.0.2",
  "main": "dist/type.min.js",
  "description": "A WYSIWYG Editing Library",
  "repository": {
    "type": "git",
    "url": "git@github.com:LukasBombach/Type.js.git"
  },
  "keywords": [
    "wysiwyg",
    "editor",
    "contenteditable",
    "rich text"
  ],
  "author": "Lukas Bombach",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "uglify-js": "*",
    "webpack": "^1.12.11"
  },
  "scripts": {
    "build": "webpack src/type.js dist/type.js --module-bind \"js=babel\" -d",
    "watch": "webpack src/type.js dist/type.js --module-bind \"js=babel\" -d --watch",
    "dist": "webpack src/type.js dist/type.js --module-bind \"js=babel\" && uglifyjs dist/type.js -o dist/type.min.js",
    "demo": "open demo/editor.html"
  }
}
